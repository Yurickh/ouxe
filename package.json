{
  "name": "ouxe",
  "version": "1.0.9",
  "description": "Opinionated configuration for common tooling.",
  "bin": {
    "ouxe": "bin/ouxe"
  },
  "main": "build/ouxe.cjs",
  "engines": {
    "node": ">=13.0.0"
  },
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "docs",
    "bin"
  ],
  "author": "Yurick <yurick.hauschild@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/Yurickh/ouxe",
  "scripts": {
    "clean-build": "rm -rf ./lib",
    "compile": "tsc -p .",
    "test": "jest",
    "lint": "eslint ./src/**/*",
    "build": "yarn clean-build && yarn compile && yarn copy-templates",
    "start": "microbundle watch",
    "copy-templates": "cp -r ./src/templates ./lib",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "covgen": "^3.1.2",
    "gluegun": "^5.1.2",
    "license": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^24.0.22",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "babel-jest": "^28.1.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^4.1.0",
    "husky": "^8.0.1",
    "jest": "^24.1.0",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^12.4.1",
    "prettier": "npm:@btmills/prettier",
    "semantic-release": "^19.0.2",
    "semantic-release-gitmoji": "^1.4.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  }
}
